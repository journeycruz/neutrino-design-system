      "h2": { "$type": "dimension", "$value": "{font.size.800}" },
      "h3": { "$type": "dimension", "$value": "{font.size.700}" },
      "h4": { "$type": "dimension", "$value": "{font.size.600}" }
    }
  },
  "layout": {
