    },
    "body": {
      "size": { "$type": "dimension", "$value": "{font.size.300}" },
      "lineHeight": { "$type": "number", "$value": "{font.lineHeight.normal}" },
      "weight": { "$type": "fontWeight", "$value": "{font.weight.regular}" }
    },
    "label": {
      "size": { "$type": "dimension", "$value": "{font.size.200}" },
      "lineHeight": { "$type": "number", "$value": "{font.lineHeight.normal}" },
      "weight": { "$type": "fontWeight", "$value": "{font.weight.medium}" }
    },
    "heading": {
